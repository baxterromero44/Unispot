<%- include ('../partials/headers_logged_in') %>


<main class="mb-48 flex flex-col">
  
  <div class="flex flex-col justify-center m-auto  mx-4">

    <div class="text-5xl mt-8 mb-16 mx-auto">Top Songs for CU Boulder:</div>
    <div class="grid grid-cols-2 gap-4 max-w-xl mx-auto">

      <% 
     if (data) { 
       <!-- console.log('444', result.dates.start) -->
       cards = '';
       count = 0;
       data.forEach((result) => {
        count++;
        <!-- if (result.popularity_score >= 3) { -->
        <!-- cards += '<div class="p-3 bg-pink-500 cursor-pointer rounded">'; -->
        <!-- } else { -->

          <!-- } -->
          <!-- console.log('@@@@',result); -->
          cards += '<div class="p-3 bg-stone-100 cursor-pointer rounded">';
        cards += '<div class="mb-4 text-4xl font-semibold italic">#'+count+'</div>';

        cards += '<img class="" src="' + result.image_link + '">';
        cards += '<div class="text-2xl font-semibold my-2">'+result.title+'</div>';

        cards += '</div>';
      })

      }; %> 

     <%- cards %> 

     
    </div>
    <div class="text-lg mt-16 text-gray-560 mx-auto underline">
      <a href="/home">Go to Home</a>
    </div>

  </div>
</main>

<%- include ('../partials/footer') %>