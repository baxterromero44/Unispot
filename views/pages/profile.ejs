<%- include ('../partials/headers_logged_in.ejs') %>
    <main>
        <div class="container">
            <h1>
                <p class="text-2xl"> Welcome User : <%= username %>
                </p>
            </h1>
            <table class="table">
                <tbody>
                    <tr>
                        <td>Username:</td>
                        <td><%= username %></td>
                    </tr>
                        <td>Password:</td>
                        <td><%= password%></td>
                    </tr>
                    </tr>
                        <td>University:</td>
                        <td><%= university_id %></td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="h-56 grid grid-cols-3 gap-4 content-end ...">
            <div> <button
                    class="bg-transparent hover:bg-green-500 text-green-700 font-semibold hover:text-white py-2 px-4 border border-green-500 hover:border-transparent rounded">
                    Change Password
                </button>
                <button
                    class="bg-transparent hover:bg-green-500 text-green-700 font-semibold hover:text-white py-2 px-4 border border-green-500 hover:border-transparent rounded">
                    Change Username
                </button>
                <button
                class="bg-transparent hover:bg-green-500 text-green-700 font-semibold hover:text-white py-2 px-4 border border-green-500 hover:border-transparent rounded">
                    Change University
                </button>
            </div>
        <div>
    </main>
    <%- include ('../partials/footer') %>